    <li class="span4">
      <div class="thumbnail">
        <div class="caption">
          <h3><%= feed.name %></h3>
          <% feed.articles.sort_by{|el| el.published}.reverse.first(5).each_with_index do |a,i| %>
            <% if i == 0 %>
              <p><b><%= link_to "#{a.title}", a.url%><%= " par #{a.author}" unless a.author.nil?%> le <%= a.published.strftime("%d/%m/%Y")%></b></p>
              <ul>
            <% else %>
              <li> <%= link_to "#{a.title}", a.url%><%= " par #{a.author}" unless a.author.nil?%> le <%= a.published.strftime("%d/%m/%Y")%> </li>
            <% end %>
          <% end %>
        </ul>
        </div>
      </div>
    </li>



